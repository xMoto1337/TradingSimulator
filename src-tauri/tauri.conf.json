{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Trading Simulator",
  "version": "1.5.0",
  "identifier": "com.moto1337x.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1421",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Trading Simulator",
        "width": 1400,
        "height": 900,
        "minWidth": 1000,
        "minHeight": 700,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "Moto1337x",
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDdGMjZFRTQzQjYyMTREN0YKUldSL1RTRzJRKzRtZjNiRDdnQ08xTjVqcVNNeDRNSlV1OGkvK3RKaVgyYjFUQkxmOHQ4Q3dKUlEK",
      "endpoints": [
        "https://raw.githubusercontent.com/xMoto1337/TradingSimulator/main/latest.json"
      ]
    }
  }
}
